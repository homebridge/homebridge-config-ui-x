{
  "name": "homebridge-config-ui-x",
  "version": "3.0.0-19",
  "description": "Configuration UI plugin for Homebridge",
  "license": "MIT",
  "keywords": [
    "homebridge-plugin"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/oznu/homebridge-config-ui-x.git"
  },
  "bugs": {
    "url": "https://github.com/oznu/homebridge-config-ui-x/issues"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "npm run build:server && npm run build:ui",
    "build:server": "tsc",
    "build:ui": "npm run build --prefix ui",
    "watch:server": "tsc --watch",
    "watch:ui": "npm run start --prefix ui"
  },
  "bin": {
    "homebridge-config-ui-x": "./bin/standalone.js"
  },
  "engines": {
    "node": ">=6.9.1",
    "homebridge": ">=0.4.22"
  },
  "dependencies": {
    "@oznu/hap-client": "0.0.3",
    "bash-color": "^0.0.4",
    "bluebird": "^3.5.1",
    "body-parser": "~1.18.2",
    "buffer-shims": "^1.0.0",
    "commander": "^2.13.0",
    "cors": "^2.8.4",
    "express": "~4.16.2",
    "fs-extra": "^5.0.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "node-pty": "^0.7.4",
    "nsp": "^3.2.1",
    "passport": "~0.4.0",
    "passport-http": "^0.3.0",
    "qr-image": "^3.2.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "semver": "^5.5.0",
    "source-map-support": "^0.5.1",
    "ws": "^4.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.11.0",
    "@types/fs-extra": "^5.0.0",
    "@types/node": "^9.3.0",
    "@types/passport": "^0.4.2",
    "@types/ws": "^4.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  }
}
