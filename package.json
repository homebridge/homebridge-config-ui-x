    "qr-image": "^3.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "ts-loader": "^8.3.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.10.1",
