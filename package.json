{
  "name": "homebridge-config-ui-x",
  "version": "3.6.6",
  "description": "Configuration UI plugin for Homebridge",
  "license": "MIT",
  "keywords": [
    "homebridge-plugin"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/oznu/homebridge-config-ui-x.git"
  },
  "bugs": {
    "url": "https://github.com/oznu/homebridge-config-ui-x/issues"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "npm run build:server && npm run build:ui",
    "build:server": "tsc",
    "build:ui": "npm run build --prefix ui",
    "watch:server": "tsc --watch",
    "watch:ui": "npm run start --prefix ui",
    "prepublishOnly": "npm i --package-lock-only && nsp audit && npm run build",
    "postinstall": "node dist/bin/fake-nsp.js"
  },
  "bin": {
    "homebridge-config-ui-x": "./dist/bin/standalone.js"
  },
  "engines": {
    "node": ">=6.9.1",
    "homebridge": ">=0.4.22"
  },
  "dependencies": {
    "@oznu/hap-client": "0.0.3",
    "bash-color": "^0.0.4",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "buffer-shims": "^1.0.0",
    "commander": "^2.15.1",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "fs-extra": "^6.0.1",
    "helmet": "^3.12.1",
    "jsonwebtoken": "^8.2.1",
    "node-pty": "^0.7.4",
    "passport": "~0.4.0",
    "passport-http": "^0.3.0",
    "qr-image": "^3.2.0",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "semver": "^5.5.0",
    "source-map-support": "^0.5.6",
    "ws": "^5.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.11.0",
    "@types/fs-extra": "^5.0.2",
    "@types/node": "^10.1.2",
    "@types/passport": "^0.4.5",
    "@types/ws": "^5.1.0",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  }
}
