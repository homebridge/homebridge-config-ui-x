    "eslint-plugin-jest": "^26.1.3",
    "form-data": "^4.0.0",
    "jest": "^27.5.1",
    "lodash": "^4.17.20",
    "nodemon": "^2.0.12",
    "otplib": "^12.0.1",
    "prettier": "^2.6.2",
    "qr-image": "^3.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^2.5.2"
  },
  "keywords": [
