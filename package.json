    "tail": "^2.2.4",
    "tar": "^6.1.11",
    "tcp-port-used": "^1.0.2",
    "unzipper": "^0.10.11"
  },
  "devDependencies": {
    "@nestjs/testing": "8.4.4",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/node-schedule": "^1.3.2",
    "@types/semver": "^7.3.8",
    "@types/tar": "^6.1.1",
    "@types/unzipper": "^0.10.5",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "bash-color": "0.0.4",
    "buffer-shims": "^1.0.0",
