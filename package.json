    "@oznu/hap-client": "1.8.0",
    "axios": "0.21.2",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "commander": "6.2.1",
    "dayjs": "^1.10.6",
    "fastify": "3.19.2",
    "fastify-multipart": "4.0.7",
    "fastify-static": "4.4.1",
    "fastify-swagger": "4.8.3",
    "fs-extra": "9.1.0",
    "helmet": "4.4.1",
    "node-cache": "^5.1.2",
    "node-pty-prebuilt-multiarch": "^0.10.1-pre.4",
    "node-schedule": "^1.3.3",
    "ora": "5.3.0",
    "p-limit": "3.1.0",
    "passport": "0.4.1",
